{
  "version": 2,
  "rewrites": [
    {
      "source": "/",
      "destination": "/web/index.html"
    },
    {
      "source": "/web",
      "destination": "/web/index.html"
    },
    {
      "source": "/web/(.*)",
      "destination": "/web/$1"
    }
  ],
  "ignoreCommand": "if [ \"$VERCEL_GIT_COMMIT_AUTHOR_LOGIN\" = \"dependabot[bot]\" ]; then exit 0; fi; exit 1"
}
